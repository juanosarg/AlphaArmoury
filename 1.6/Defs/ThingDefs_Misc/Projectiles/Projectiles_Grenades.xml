<Defs>
	<ThingDef ParentName="AArmoury_BaseExplosiveProjectile">
		<defName>AArmoury_AcidGrenade</defName>
		<label>acid grenade</label>
		<graphicData>
			<texPath>Things/Projectile/AArmoury_AcidGrenade</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>1</drawSize>
		</graphicData>
		<projectile>
			<flyOverhead>false</flyOverhead>
			<damageDef>AArmoury_AcidSpit</damageDef>
			<damageAmountBase>8</damageAmountBase>
			<explosionRadius>2.5</explosionRadius>
