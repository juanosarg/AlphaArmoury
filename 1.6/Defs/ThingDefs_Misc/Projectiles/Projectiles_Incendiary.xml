<Defs>
	
	<ThingDef ParentName="BaseBullet">
		<defName>AArmoury_Incendiary_Revolver</defName>
		<label>incendiary revolver bullet</label>
		<graphicData>
			<texPath>Things/Projectile/AArmoury_Incendiary</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>0.5</drawSize>
		</graphicData>
		
		<projectile>
			<damageDef>Bullet</damageDef>
			<damageAmountBase>12</damageAmountBase>
